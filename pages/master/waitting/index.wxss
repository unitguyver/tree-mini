/* pages/master/waitting/index.wxss */
.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.content{
  width: 100%;
  height: 1rpx;
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}

.switch{
  width: 100%;
  height: 60rpx;
  flex-shrink: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.search{
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.students{
  width: 100%;
  height: 1rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}

.footer{
  width: 100%;
  height: 60rpx;
  flex-shrink: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}